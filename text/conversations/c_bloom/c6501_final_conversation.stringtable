      <ID>133</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>134</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>135</ID>
      <DefaultText>"Ich schätze, es ist Zeit zu gehen."</DefaultText>
      <FemaleText />
    </Entry>
