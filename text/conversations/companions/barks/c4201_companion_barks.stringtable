    <Entry>
      <ID>20</ID>
      <DefaultText>"Wartet! Da kommt jemand... Bereitet euch auf einen Kampf vor!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>21</ID>
      <DefaultText>"Mein siebter Sinn warnt mich vor *Gefahr*! Unterwegs! *Wunderbar*!"</DefaultText>
      <FemaleText />
