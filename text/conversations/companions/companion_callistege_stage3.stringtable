    <Entry>
      <ID>7</ID>
      <DefaultText>"Ich möchte dich zu einem unserer Begleiter befragen."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText>"Wir sollten uns trennen."</DefaultText>
      <FemaleText />
