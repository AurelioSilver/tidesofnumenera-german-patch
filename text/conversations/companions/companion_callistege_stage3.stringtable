    <Entry>
      <ID>125</ID>
      <DefaultText>"Was ist deine Geschichte?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>134</ID>
      <DefaultText>"Ich möchte nochmal über dich sprechen."</DefaultText>
      <FemaleText />
    </Entry>
