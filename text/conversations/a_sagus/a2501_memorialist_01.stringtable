<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2501_memorialist_01</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Oh. Du hast mich erschreckt. Ich bin es nicht gewohnt unter so vielen Menschen zu sein!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Wir werden in Kürze wieder ins Tal der toten Helden aufbrechen. Glücklicherweise."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Ich hoffe, dass sie sich um mein Grab gekümmert haben!"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Du solltest einmal das Tal der toten Helden besuchen. Es ist ganz nett, auf seine Art."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Lebe wohl!"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>