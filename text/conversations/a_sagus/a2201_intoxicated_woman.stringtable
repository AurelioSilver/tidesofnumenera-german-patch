<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2201_intoxicated_woman</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>„Fäden. Fetzen. Sehnen, die sich beugen.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>„Wenn ich nicht aufhöre. Wenn ich nicht *aufhöre*, höre ich nicht … höre ich nicht das …“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>„Höre das Singen. Der Chor der Schreie. Wunden in der Luft. Schorf, der niemals heilt!“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>„Er wartet. Er ist das. Er ist immer da. Bedrängend. Sich anbahnend! Eine *Krankheit* der Haut!“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>„Schwarze Brühe in meinen Adern. In meinem Hals. In meinen Augen.“</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>