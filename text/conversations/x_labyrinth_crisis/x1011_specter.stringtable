    <Entry>
      <ID>83</ID>
      <DefaultText>„Führe einfach diese Einstellungssache durch. Ich weiß nicht, wie viel Zeit uns noch bleibt.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>84</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>85</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>86</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>87</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>88</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>89</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>90</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
