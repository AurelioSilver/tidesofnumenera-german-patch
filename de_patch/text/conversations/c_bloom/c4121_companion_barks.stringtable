    <Entry>
      <ID>13</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>14</ID>
      <DefaultText>„Ich werde dort nicht hineingehen. Das ist zu sehr wie verschluckt werden.“</DefaultText>
      <FemaleText />
    </Entry>
