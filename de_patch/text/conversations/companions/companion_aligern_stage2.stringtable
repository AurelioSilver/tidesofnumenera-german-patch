    <Entry>
      <ID>104</ID>
      <DefaultText>„Ich denke, wir sollten uns trennen.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>105</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>106</ID>
