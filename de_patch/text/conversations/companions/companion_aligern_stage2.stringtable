      <ID>79</ID>
      <DefaultText>„Wie geht es dir?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>80</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>81</ID>
      <DefaultText />
