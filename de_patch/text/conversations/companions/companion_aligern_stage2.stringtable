    <Entry>
      <ID>386</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>387</ID>
      <DefaultText>„Warum gehst du nicht einfach?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>388</ID>
      <DefaultText>„Warum kümmerst du dich nicht um deine eigenen Sachen?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>389</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>390</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
