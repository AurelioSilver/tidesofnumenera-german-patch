<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\a_sagus\a2201_ambient_thug_02</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>5</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>„Folgst du mir aus einem bestimmten Grund?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>„Sehe ich aus wie einer, dem man folgen sollte?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>„Ich erschrecke mich leicht, wenn andere mir folgen. Geht ein Licht auf?“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>„Der Letzte, der mir folgte, bekam einen Dolch in den Rücken. War natürlich ein Unfall.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>(seufzt) „Sicher. Folge mir einfach. Das versüßt mir den Tag.“</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>